    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "core-js": "^3.14.0",
    "less": "^4.1.1",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.4",
